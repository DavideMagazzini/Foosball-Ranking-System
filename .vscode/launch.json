{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python Debuggamelo: Current File",
      "type": "debugpy",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal",
      "env": {
                "PYTHONPATH": "${workspaceFolder}"
            }
    },
    {
      "type": "chrome",
      "request": "launch",
      "name": "Debug Flask App in Chrome",
      "url": "http://127.0.0.1:5000",
      "webRoot": "${workspaceFolder}/templates"
    }
  ]
}